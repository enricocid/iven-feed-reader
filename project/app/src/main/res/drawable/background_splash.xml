<?xml version="1.0" encoding="utf-8"?>

<!-- googlish splash screen -->

<layer-list android:opacity="opaque"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@android:color/white" />
    <item android:bottom="@dimen/launchscreens_iven_icon_bottom">
        <bitmap android:gravity="center" android:src="@drawable/iven_icon" />
    </item>
    <item android:bottom="@dimen/launchscreens_iven_logo_bottom">
        <bitmap android:gravity="bottom|center" android:src="@drawable/iven_logo_dark" />
    </item>
</layer-list>